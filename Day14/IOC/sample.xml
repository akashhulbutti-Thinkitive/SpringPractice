<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
        http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="emp" class="com.akash.Employee">
		<!-- <constructor-arg name="empId" value="1"></constructor-arg> <constructor-arg 
			name="empName" value="AKash"></constructor-arg> <constructor-arg name="salary" 
			value="550000"></constructor-arg> -->
		<property name="empId" value="2"></property>
		<property name="empName" value="Akash"></property>
		<property name="salary" value="850000"></property>
		<property name="projects">
			<list>
				<value>ERP1</value>
				<value>ERP2</value>
				<value>MediCare</value>
			</list>
		</property>
	</bean>
	<bean id="addr" class="com.akash.Address"></bean>
</beans>